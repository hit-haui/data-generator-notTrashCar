cd catkin_ws
rm -rf devel/
catkin_make
source devel/setup.bash
sudo chown root:root ./devel/lib/car_controller/car_controller_node
sudo chmod a+rx ./devel/lib/car_controller/car_controller_node
sudo chmod u+s ./devel/lib/car_controller/car_controller_node
sudo chown root:root ./devel/lib/hal/hal_node
sudo chmod a+rx ./devel/lib/hal/hal_node
sudo chmod u+s ./devel/lib/hal/hal_node
sudo chown root:root ./devel/lib/hal/pub
sudo chmod a+rx ./devel/lib/hal/pub
sudo chmod u+s ./devel/lib/hal/pub

